#banner .content .image img {
    height: 100%;
    object-fit: cover;
}

body.landing #header {
    position: fixed;
    background-color: #000;
}

.video-container {
    position: relative;
    margin-left: auto;
    margin-right: auto;
    width: 80%;
    padding-bottom: 56.25%;
    padding-top: 30px;
    height: 0;
    overflow: hidden;
}

.video-container iframe, .video-container object, .video-container embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }


ul.icons li .icon {
    font-size: 2em;
}

#breadcrumbs {
    margin-top: 1rem;
    margin-bottom: 1rem;
}